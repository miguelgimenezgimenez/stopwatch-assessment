{
    "extends": [
        "airbnb",
        "standard"
    ],
    "globals": {
        "test": true,
        "expect": true
    },
    "rules": {
        "object-curly-newline": 0,
        "class-methods-use-this": 0,
        "lines-between-class-members": 2,
        "max-len": [
            2,
            120
        ],
        "no-console": 2,
        "import/prefer-default-export": false,
        "react/prefer-stateless-function": false,
        "react/jsx-filename-extension": false,
        "import/no-extraneous-dependencies": false,
        "react/prop-types": false,
        "consistent-return": 0,
        "react/no-array-index-key": false,
        "import/no-absolute-path": false,
        "import/extensions": false,
        "no-plusplus": "off",
        "jsx-a11y/media-has-caption": false,
        "jsx-a11y/click-events-have-key-events": false,
        "jsx-a11y/no-static-element-interactions": false,
        "jsx-a11y/tabindex-no-positive": false,
        "jsx-a11y/no-noninteractive-tabindex": false,
        "react/sort-comp": false,
        "react/no-did-update-set-state": false
    }
}
